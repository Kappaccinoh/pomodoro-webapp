[run]
source = pomodorobackend
omit =
    */migrations/*
    */tests/*
    */wsgi.py
    */asgi.py
    manage.py
    */settings.py
    */__init__.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError 